{
    "Statement": [
        {
            "Action": "s3:*",
            "Condition": {
                "Bool": {
                    "aws:SecureTransport": "false"
                }
            },
            "Effect": "Deny",
            "Principal": "*",
            "Resource": "arn:aws:s3:::${bucket_name}/*",
            "Sid": "DenyInsecureTransport"
        }
    ],
    "Version": "2012-10-17"
}
